<?xml version='1.0'?>
<application>

	<definition>
		<idname>innowork-faq</idname>
		<release>
			<version>1.1</version>
			<date>2014/01/02</date>
			<changesfile>CHANGES</changesfile>
		</release>
		<description>Innowork FAQ</description>
		<category>intranet</category>
		<iconfile></iconfile>
		<dependencies>
			<dependency>innowork-core[3.1]</dependency>
		</dependencies>
		<options>
			<!-- <option></option> -->
		</options>
		<legal>
			<author>
				<name>Innoteam Srl</name>
				<email>info@innoteam.it</email>
				<web>http://www.innoteam.it/</web>
			</author>
			<copyright>Copyright (c) 2003-2014 Innoteam Srl</copyright>
			<license>Commercial</license>
			<licensefile>LICENSE</licensefile>
		</legal>
		<support>
			<supportemail>helpdesk@innoteam.it</supportemail>
			<bugsemail>helpdesk@innoteam.it</bugsemail>
			<maintainer>
				<name>Innoteam Srl</name>
				<email>helpdesk@innoteam.it</email>
			</maintainer>
		</support>
	</definition>

	<components>

		<domaingroup name="support"
			catalog="innowork-faq::innoworkfaq_misc" />

        <catalog name="innowork-faq" />

		<domainpanel name="innoworkfaq" file="innoworkfaq.php"
			category="support" themeicon="info" themeicontype="apps"
			catalog="innowork-faq::innoworkfaq_misc" />

		<domaintable name="innowork_faq_categories"
			file="innowork_faq_categories.xml" />
		<domaintable name="innowork_faq_nodes"
			file="innowork_faq_nodes.xml" />

		<class name="innowork/faq/InnoworkFaqCategory.php" />
		<class name="innowork/faq/InnoworkFaqNode.php" />

		<innoworkitemtype name="faqcategory" file="faqcategory"
			catalog="innowork-faq::innoworkfaq_misc" classfile="innowork/faq/InnoworkFaqCategory.php"
			classname="InnoworkFaqCategory" icon="info" icontype="apps"
			domainpanel="innoworkfaq" miniicon="info"
			summaryname="faqcategory-name" />

		<innoworkitemtype name="faqnode" file="faqnode"
			catalog="innowork-faq::innoworkfaq_misc" classfile="innowork/faq/InnoworkFaqNode.php"
			classname="InnoworkFaqNode" icon="info" icontype="apps"
			domainpanel="innoworkfaq" miniicon="info"
			summaryname="faqnode-name" showmode="app" />

	</components>
</application>
